{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"],
      "@/styles/*": ["./src/styles/*"],
      "@/lib/*": ["./src/lib/*"],
      "@/components/*": ["./src/components/*"],
      "@/ui/*": ["./src/components/ui/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/components/Marketverse/camera.jsx", "src/components/Marketverse/renderer.jsx", "src/components/Marketverse/textsprite.jsx", "src/components/Marketverse/pointlight.jsx", "src/components/Marketverse/particles.jsx", "src/components/Marketverse/eventhandler.jsx", "src/components/Marketverse/ambientlight.jsx", "src/prisma/seed.jsx", "src/pages/api/cron.jsx"],
  "exclude": ["node_modules"]
}
